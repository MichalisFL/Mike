#include <iostream>

using namespace std;

void insertion_sort(char arr[], int length)
{
	int i, j, tmp;
	for (i = 1; i < length; i++) {
		j = i;
		while (j > 0 && arr[j - 1] > arr[j]) {
			tmp = arr[j];
			arr[j] = arr[j - 1];
			arr[j - 1] = tmp;
			j--;
		}
	}
}

void print_array(char array[], int size)
{
	int j;
	for (j = 0; j < size; j++)
	{
		cout << array[j] << endl;
	}
}

int main()
{
	char array[] = { 'A', 'R', 'E' };
	insertion_sort(array, 3);
	print_array(array, 3);
	return 0;
}
