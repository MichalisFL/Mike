#include <iostream>

using namespace std;

void sort(char input[], int size){
	int i, j, temp;
	for (i = 1; i < size; i++){
		j = i;
		while (j>0 && input[j - 1] > input[j]){
			tmp = input[j - 1];
			input[j] = input[j - 1];
			input[j - 1] = tmp;
			j--;
		}
	}
}
int main() {
	cout << "Give size of array: ";
	cin >> size[];





}
