#include <iostream>

using namespace std;

void sort(char input[], int size){
	int i, j;
	for (i = 1; i < size; i++){
		int tmp = input[i];
		for (j = i; j > 0 && input[j - 1] > input[j]; j--) { // Condition
			tmp = input[j];
			input[j] = input[j - 1];
		}
		input[j] = tmp;
	}
}

void print_input(char input[], int size){

	for (int j = 0; j < size; j++){
		cout << size[j] << endl;
	}
}
int main() {
	cout << "Give size of array: ";
	cin >> size[];






}
